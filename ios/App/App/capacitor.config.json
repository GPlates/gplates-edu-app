{
	"appId": "io.ionic.starter",
	"appName": "gplates-edu-ion",
	"webDir": "build",
	"bundledWebRuntime": false,
	"server": {
		"cleartext": true
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 3000,
			"launchAutoHide": true,
			"backgroundColor": "#000000",
			"androidSplashResourceName": "splash",
			"androidScaleType": "CENTER",
			"showSpinner": false,
			"layoutName": "launch_screen"
		}
	}
}
